.data-header
	.row.padding-top-1
	  	.col
	    	.alert.alert-primary
	        	.alert-heading
	          		.row
	          			.col
	          				| TRANSAKSI
	          			.col.text-right
	          				= page_entries_info @transactions
	   	
	  	- if buttonAccess "transactions", "new"
		    - if current_user.store.online_store
			  	.col-2.text-center
				    a.btn.btn-dark.btn-rounded href="#{new_transaction_path}"
				      	i.fas.fa-plus 
				      	|  TRANSAKSI

	- if isLevel ["super_visi", "finance", "candy_dream"]
		table.table.table-hover 
		  thead.text-left
		    th width="30%" Invoice
		    th width="25%" Tanggal
		    th width="25%" Toko
		    th.text-right width="20%" Nominal

- if isLevel ["super_visi", "finance", "candy_dream"]
	table.table.table-hover
	  tbody
	    = render partial: 'list', collection: @transactions, as: :transaction
	.container.pagination.justify-content-center
  		= paginate @transactions

  		
